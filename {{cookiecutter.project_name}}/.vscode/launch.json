{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": ["runserver_plus", "0.0.0.0:8000"],
            "django": true,
            "autoStartBrowser": false,
            "env": {
                "LOG_DB": "true"
            },
            "program": "${workspaceFolder}/manage.py"
        }
    ]
}
